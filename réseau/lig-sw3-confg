


hostname LIG-SW3

no logging console

service password-encryption
enable secret cisco
username admin privilege 15 password admin

no ip domain-lookup
ip domain-name m2l.lan
ip name-server 172.16.2.61

crypto key generate rsa
1024

ip ssh time-out 60
ip ssh authentication-retries 2
ip ssh version 2

vtp domain m2l.lan
vtp mode transparent



vlan 10
 name VLAN_TENNIS

vlan 11
 name VLAN_ATHLE

vlan 12
 name VLAN_BASKET

vlan 99
 name VLAN_MGMT

interface Port-channel1
 switchport mode trunk

interface Port-channel2
 description INTERCO_LIG_SW1
 switchport mode trunk

interface FastEthernet0/1
 description INTERCO_LIG_SW1
 switchport mode trunk
 channel-group 2 mode active

interface FastEthernet0/2
 description INTERCO_LIG_SW1
 switchport mode trunk
 channel-group 2 mode active

interface FastEthernet0/3
 description INTERCO_LIG-SW2
 switchport mode trunk

interface Vlan99
 description VLAN_MGMT
 ip address 172.16.99.20 255.255.255.248


ip default-gateway 172.16.99.17
ip http server
ip http authentication local

snmp-server enable traps stpx
snmp-server enable traps rtr
snmp-server enable traps c2900
snmp-server enable traps vtp
snmp-server enable traps vlancreate
snmp-server enable traps vlandelete
snmp-server enable traps port-security
snmp-server enable traps MAC-Notification
snmp-server enable traps envmon fan shutdown supply temperature status
snmp-server enable traps hsrp
snmp-server enable traps cluster
snmp-server enable traps vlan-membership
snmp-server host 172.16.2.59 EyesOfNetwork

line con 0
line vty 0 4
 password cisco
 login local
 transport input ssh
end


